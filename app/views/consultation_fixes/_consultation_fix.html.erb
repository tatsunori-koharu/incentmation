<% if consultation.liked_by?(current_user) %>
  <p><span><%= link_to image_tag("good_black.png", class:"good"), consultation_consultation_fix_path(consultation_id: @consultation.id, id: consultation.consultation_fixes[0]), method: :delete, remote: true %></span></p>
<% else %>
  <p><span><%= link_to image_tag("good_white.png", class:"good"), consultation_consultation_fixes_path(@consultation.id), method: :post, remote: true %></span></p>
<% end %>