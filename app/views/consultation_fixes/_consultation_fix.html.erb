<% if consultation.liked_by?(current_user) %>
  <p><span><%= link_to "解決済み", consultation_consultation_fix_path(consultation_id: @consultation.id, id: consultation.consultation_fixes[0]), method: :delete, remote: true, class:"sol btn btn-success btn-lg" %></span></p>
<% else %>
  <p><span><%= link_to "未解決", consultation_consultation_fixes_path(@consultation.id), method: :post, remote: true, class:"unsol btn btn-danger btn-lg" %></span></p>
<% end %>