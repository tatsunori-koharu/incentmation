<% if article.liked_by?(current_user) %>
  <p><span><%= link_to image_tag("good_black.png", class:"good"), article_article_like_path(article_id: @article.id, id: article.article_likes[0]), method: :delete, remote: true %>:<%= article.article_likes.count %></span></p>
<% else %>
  <p><span><%= link_to image_tag("good_white.png", class:"good"), article_article_likes_path(@article.id), method: :post, remote: true %>:<%= article.article_likes.count %></span></p>
<% end %>