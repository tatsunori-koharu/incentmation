<div class="chat">
  <%= render partial: "new"%>
  <div class="chat-main">
    <h1 class="text-center">
      <%= @chat.users.where.not(nickname: current_user.nickname).pluck(:nickname).join(", ") %>
    </h1>

    <div class=" d-flex justify-content-center">
      <%= render partial: "chat_messages/message" %>
    </div>
</div>
</div>