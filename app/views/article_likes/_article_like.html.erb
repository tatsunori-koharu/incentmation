<% if article.liked_by?(current_user) %>
  <p><span><img src="images/good_black.jpeg" class= "good" >：</span><%= link_to article.article_likes.count, article_article_like_path(article), method: :delete, remote: true %></p>
<% else %>
  <p><span><img src="images/good_white.jpeg" class= "good" >：</span><%= link_to article.article_likes.count, article_article_likes_path(article), method: :post, remote: true %></p>
<% end %>